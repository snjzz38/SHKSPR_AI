{
  "version": 2,
  "builds": [
    {
      "src": "api/Grader_API.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/Humanizer_API.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/Flashcard_API.js",
      "use": "@vercel/node"
    },
    {
      "src": "/**",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/api/humanizer",
      "destination": "/api/Humanizer_API.js"
    },
    {
      "source": "/api/Grader_API",
      "destination": "/api/Grader_API.js"
    },
    {
      "source": "/api/Flashcard_API",
      "destination": "/api/Flashcard_API.js"
    },
    {
      "source": "/api/Citation_API",
      "destination": "/api/Citation_API.js"
    },
    {
      "source": "/",
      "destination": "/index.html"
    }
  ]
}
