{
  "version": 2,
  "functions": {
    "api/Grader_API.js": {
      "maxDuration": 30
    },
    "api/Humanizer_API.js": {
      "maxDuration": 30
    },
    "api/Flashcard_API.js": {
      "maxDuration": 30
    },
    "api/Quiz_API.js": {
      "maxDuration": 30
    }
  },
  "builds": [
    {
      "src": "api/Grader_API.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/Humanizer_API.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/Flashcard_API.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/Quiz_API.js",
      "use": "@vercel/node"
    },
    {
      "src": "/**",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/api/humanizer",
      "destination": "/api/Humanizer_API.js"
    },
    {
      "source": "/api/grader",
      "destination": "/api/Grader_API.js"
    },
    {
      "source": "/api/flashcard",
      "destination": "/api/Flashcard_API.js"
    },
    {
      "source": "/api/quiz",
      "destination": "/api/Quiz_API.js"
    },
    {
      "source": "/",
      "destination": "/index.html"
    }
  ]
}
