{
  "builds": [
    {
      "src": "/AI_Humanizer/api/Humanizer_API.js",
      "use": "@vercel/node"
    },
    {
      "src": "/**",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/api/humanizer",
      "destination": "/AI_Humanizer/api/Humanizer_API.js"
    },
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    }
  ]
}
