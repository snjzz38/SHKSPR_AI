{
  "version": 2,
  "builds": [
    {
      "src": "api/Grader_API.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/Humanizer_API.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/Flashcard.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/api/humanizer",
      "destination": "/api/Humanizer_API.js"
    },
    {
      "source": "/api/grader",
      "destination": "/api/Grader_API.js"
    },
    {
      "source": "/api/flashcard",
      "destination": "/api/Flashcard.js"
    },
    {
      "source": "/",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/Flashcard.js": {
      "runtime": "nodejs18.x"
    },
    "api/Grader_API.js": {
      "runtime": "nodejs18.x"
    },
    "api/Humanizer_API.js": {
      "runtime": "nodejs18.x"
    }
  },
  "env": {
    "FLASHCARD_1": "@flashcard_1"
  }
}
