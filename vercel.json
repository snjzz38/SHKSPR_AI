{
  "builds": [
    {
      "src": "/api/Grader_API.js",
      "use": "@vercel/node"
    },
    {
      "src": "/api/Humanizer_API.js",
      "use": "@vercel/node"
    },
    {
      "src": "/**",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/",
      "destination": "/index.html"
    }
  ]
}
